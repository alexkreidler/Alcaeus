{
  "name": "heracles",
  "version": "0.1.2-pre01",
  "description": "Hydra Core hypermedia-aware client library",
  "main": "dist/heracles.js",
  "directories": {
    "test": "tests"
  },
  "typescript": {
    "definition": "index.d.ts"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run",
    "build": "jspm bundle-sfx src/heracles - jsonld - li - rdf-ext - rdf-parser-jsonld - rdf-serializer-jsonld dist/heracles.js --format cjs --skip-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wikibus/heracles"
  },
  "keywords": [
    "ajax",
    "hydra",
    "semanticweb",
    "hypermedia",
    "rdf",
    "json-ld"
  ],
  "author": "Tomasz Pluskiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wikibus/heracles/issues"
  },
  "homepage": "https://github.com/wikibus/heracles#readme",
  "jspm": {
    "registry": "npm",
    "format": "cjs",
    "directories": {
      "test": "tests"
    },
    "dependencies": {
      "jsonld": "^0.4.6",
      "li": "^1.0.1",
      "rdf-ext": "^0.3.0",
      "rdf-parser-jsonld": "^0.3.0",
      "rdf-serializer-jsonld": "^0.3.0"
    },
    "devDependencies": {
      "core-decorators": "^0.12.2",
      "core-js": "^2.4.1",
      "lodash": "^4.16.4",
      "rdf-parser-n3": "^0.3.0",
      "sinon": "^1.17.3",
      "typescript": "^1.8.9",
      "whatwg-fetch": "^1.0.0"
    },
    "buildConfig": {
      "transpileES6": true,
      "minify": false
    }
  },
  "dependencies": {
    "jsonld": "^0.4.6",
    "li": "^1.0.1",
    "rdf-ext": "^0.3.0",
    "rdf-parser-jsonld": "^0.3.0",
    "rdf-serializer-jsonld": "^0.3.0"
  },
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sauce-launcher": "^1.0.0",
    "karma-systemjs": "^0.14.0",
    "sinon": "^1.17.3",
    "systemjs": "^0.19.31",
    "typings": "^1.4.0",
    "typescript": "^1.8.9",
    "whatwg-fetch": "^1.0.0"
  }
}
