{
  "presets": [
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": true
      }
    ],
    [
      "@babel/proposal-class-properties"
    ],
    "@babel/proposal-object-rest-spread",
    [
      "babel-plugin-add-import-extension",
      {
        "extension": "mjs"
      }
    ]
  ]
}
